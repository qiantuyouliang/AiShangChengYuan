/* miniprogram/pages/CreateForum/CreateForum.wxss */
.Navtxt{
  text-align: center;
  padding-top:20rpx;
}
.Container{
  padding:40rpx 20rpx 40rpx 20rpx ;
}
.Container .ForumTitle{
  font-size: 34rpx;
  height: 70rpx;
  padding: 20rpx 0rpx 0rpx 10rpx;
  margin-bottom: 30rpx;
  margin-top: 20rpx;
  background-color:  #e4e4e4;
}
.ForumTitle .Title{
  width: 100%;
  height: 70rpx;
}
.Container .Price{
  width: 30%;
  font-size: 32rpx;
  margin-bottom: 20rpx;
  height: 60rpx;
  margin-bottom: 30rpx;
  margin-top: 20rpx;
  padding: 20rpx 0rpx 0rpx 10rpx;
  background-color:  #e4e4e4;
}
.ContentGroup{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color:  white ;
  margin-bottom: 30rpx;
}
.ForumContent{
  width: 100%;
}
.Container .Content{
  box-sizing: border-box;
  width: 100%;
  padding: 10rpx 10rpx 10rpx 10rpx;
  font-size: 32rpx;
  height: 250rpx;
  background-color: #e4e4e4;
}

.AddIconGroup{
          width: 100%;
        display: flex;
 flex-direction: row;
 justify-content: space-around;
}
.AddIconGroup image{
  width: 60rpx;
  height: 60rpx;
}
.AddIconGroup text{
  font-size: 20rpx;
  padding-top:5rpx ;
  color: #1296db;
}
.TextAddIcon{
  width: 100rpx;
  height: 100rpx;
  display: flex;
  flex-direction:column;
  align-items: center;
  font-size: 20rpx;
}
.TextAddIcon image{
  padding: 2rpx;
  width: 55rpx;
  height: 55rpx;
}


.ImageAddIcon{
  width: 100rpx;
  height: 100rpx;
  display: flex;
  flex-direction:column;
  align-items: center;
  font-size: 20rpx;
}
.VideoAddIcon{
  width: 100rpx;
  height: 100rpx;
  display: flex;
  flex-direction:column;
  align-items: center;
  font-size: 20rpx;
}
.VideoAddIcon image{
  width: 60rpx;
  height: 60rpx;
}
.Content-TempImages{
  padding:10rpx 0rpx 20rpx 0rpx ;
  margin-bottom: 10rpx;
  width: 100%;
  height: 250rpx;
  background-color: #e4e4e4;
}
.Container .ImageGroupList{
  padding-top:30rpx;
  display: flex;
  flex-wrap:wrap;
  justify-content: flex-start;
}
.ImageGroupList .ImageGroup{
  
  position: relative;
}
.ImageGroup image{
  width: 100%;
  height: 100%;
}
.ImageGroup .close-btn{
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  right: -30rpx;
  top:-30rpx;
  background-size: 50rpx;
  background-repeat: no-repeat;
  background-position:center ;
  z-index: 1;
  background-image: url("data:image/png;base64,");
}
.ImageGroupList .add-image-btn{
  background-color:#e0dfdf;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding-bottom: 20rpx;
}
.Plus{
  height: 200rpx;
  padding-bottom: 40rpx;
  font-size: 200rpx;
  color: #a9a9a9;
}
.addtxt{
  font-size: 24rpx;
  color: #a9a9a9;
}
.add-image-btn view{
  margin-top:-25rpx
}
.SubmitButton{
  margin-top: 100rpx;
}